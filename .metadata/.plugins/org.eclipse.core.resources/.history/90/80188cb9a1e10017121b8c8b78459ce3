
public class Main {
	public static void main(String[] args) {
		algo(5);
	}
	
	
	
	public static void algo(int val) {
		int input = val;
		boolean test = false;
		
		if(input % 2 == 1) {
			test = true;
			input -= 1;
		}

		System.out.println(input + ", ");
		
		if(input > 3) {
			algo(input - 2);
		}
		
		if(input == 2) {
			System.out.println(1 + " ");
		}
		
		System.out.println( ", " + input + 1);
	}
}
